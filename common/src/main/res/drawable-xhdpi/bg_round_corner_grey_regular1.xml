<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_pressed="false">
    <shape android:shape="rectangle">
        <corners android:radius="@dimen/size_corner_radius_regular"/>
        <stroke android:width="1dp"
            android:color="@color/color_app_divider"/>

        <padding
            android:left="@dimen/dimen_app_padding_large"
            android:top="@dimen/dimen_app_padding_large"
            android:right="@dimen/dimen_app_padding_large"
            android:bottom="@dimen/dimen_app_padding_large" />
    </shape>
</item>
</selector>